pipeline {
    
    agent any
    
    stages{
        
        stage('clone'){
            steps{
                echo 'cloning repo'
                
            }
            
        }
        stage ('test'){
            steps{
                echo 'testing code'
            }
        }
        stage('deploy'){
            steps{
                echo 'deploying code'
            }
            
        }
    }
}
